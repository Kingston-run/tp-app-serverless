{
  "routes": [
    {
      "route": "/api/*",
      "rewrite": "https://tp-apim-serverless.azure-api.net/api/{*}"
    }
  ],
  "globalHeaders": {
    "Access-Control-Allow-Origin": "*",
    "Access-Control-Allow-Methods": "GET, POST, OPTIONS, PUT, DELETE",
    "Access-Control-Allow-Headers": "Content-Type, Authorization"
  },
  "navigationFallback": {
    "rewrite": "/index.html"
  }
}
